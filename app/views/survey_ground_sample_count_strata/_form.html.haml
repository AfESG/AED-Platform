= semantic_form_for @survey_ground_sample_count_stratum do |f|
  = render "shared/error_messages", :target => @survey_ground_sample_count_stratum
  = f.inputs do
    = f.input :survey_ground_sample_count_id, :as => :hidden, :value => params[:survey_ground_sample_count_id]
    = f.input :stratum_name
    = f.input :stratum_area
    = f.input :population_estimate
    = f.input :population_variance
    = f.input :population_standard_error
    = f.input :population_t
    = f.input :population_degrees_of_freedom
    = f.input :population_confidence_limits
    = f.input :population_no_precision_estimate_available
    = f.input :transects_covered
    = f.input :transects_covered_total_length
    = f.input :person_hours
  = f.buttons
